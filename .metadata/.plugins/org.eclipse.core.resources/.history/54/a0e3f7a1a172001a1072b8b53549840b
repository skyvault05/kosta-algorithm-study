package algorithm_study;

public class SimpleTest {

	public static void main(String[] args) {
		int n=20;
		char[] cr = new char[n];
		  System.out.println(cr[2]);
	      for(int i=0; i<n; i+=2){
	          cr[i]='수';
	      }
	      for(int i=1; i<n; i+=2){
	          cr[i]='박';
	      }
		
		
	}

}
