package algorithm_study.ex0406;

import java.util.HashMap;
import java.util.Map;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Solution {
	public int[] solution(int N, int[] stages) {
        int[] answer = {};
        Map<Integer, Integer> map = new HashMap();
        int val = 0;
        for(int i=1; i<=stages.length; i++){
        	if(map.get(stages[i-1])==null) {
        		val = 1;
        	} else {
        		val = map.get(stages[i-1])+1;
        	}
        	
            map.put(stages[i-1], val);
        }
        Stream<Map.Entry<Integer, Integer>> sorted =
        	    map.entrySet().stream()
        	       .sorted(Map.Entry.comparingByValue()).toArray();
        
        
        
        
        
        
//        for(int aa : stages) {
//        	System.out.print(aa+":");
//        }
//        System.out.println();
//        for(int aa : hash.keySet()) {
//        	
//        	System.out.println(aa);
//        }
        return answer;
    }

	public static void main(String[] args) {
		Solution s = new Solution();
		int[] i1 = {2, 1, 2, 6, 2, 4, 3, 3};
		s.solution(5, i1);
	}

}
