package algorithm_study;  rtgrffggtgg

public class SimpleTest {

	public static void main(String[] args) {
		int n=20;
		char[] cr = new char[n];
		System.out.println(new String(new char [n/2+1]).replace("\0", "수박").substring(0,n));
	      for(int i=0; i<n; i+=2){
	          cr[i]='수';
	      }
	      for(int i=1; i<n; i+=2){
	          cr[i]='박';
	      }
		
		
	}

}
